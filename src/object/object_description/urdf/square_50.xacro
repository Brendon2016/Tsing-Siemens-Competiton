<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="square_50" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="mass" value="0.07" /> <!-- arbitrary value for mass -->

  <xacro:include filename="$(find object_description)/urdf/materials.xacro" />

  <link name="world"/>

  <joint name="world_Square_50" type="floating">
    <parent link="world"/>
    <child link="square_50"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>


  <!-- table -->
  <link name="square_50">

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	<mesh filename="package://object_description/meshes/square_50.stl" scale="1 1 1" />
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	<mesh filename="package://object_description/meshes/square_50.stl" scale="1 1 1" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	  ixx="0.00000364583333" ixy="0.0" ixz="0.0"
	  iyy="0.00000364583333" iyz="0.0"
	  izz="0.000003125" />
    </inertial>
  </link>

  <gazebo reference="square_50">  
    <material>Gazebo/Wood</material>  
	<mu1>3000</mu1>  
    <mu2>200</mu2>  
    <kp>8000.0</kp> 
    <kd>1.0</kd>  
  </gazebo> 


</robot>
